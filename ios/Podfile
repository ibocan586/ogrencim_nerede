ENV['COCOAPODS_NO_REPOS_UPDATE'] = 'true'
platform :ios, '14.0'

use_frameworks!
use_modular_headers!

flutter_application_path = File.expand_path('..', __dir__)
load File.join(flutter_application_path, '.ios', 'Flutter', 'podhelper.rb')

target 'Runner' do
  install_all_flutter_pods(flutter_application_path)
end
